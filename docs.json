    }
  },
  "seo": {
    "metatags": {
      "title": "CoinPaprika API Docs | Cryptocurrency Market Data API",
      "description": "Official documentation for the CoinPaprika API. Access real-time and historical cryptocurrency market data, including prices, volumes, market caps, and more.",
      "keywords": "CoinPaprika, CoinPaprika API, cryptocurrency API, crypto market data API, blockchain data API, digital asset data, real-time crypto prices, historical crypto prices, token metadata API, crypto data for developers, REST crypto API, JSON crypto feed, web3 data API",
      "robots": "index, follow, max-image-preview:large, max-snippet:-1, max-video-preview:-1",
      "og:type": "website",
      "og:site_name": "CoinPaprika",
      "og:title": "CoinPaprika API Docs | Cryptocurrency Market Data API",
      "og:description": "API reference and guides for accessing real-time & historical crypto data from CoinPaprika.",
      "og:url": "https://docs.coinpaprika.com",
      "twitter:card": "summary_large_image",
      "twitter:title": "CoinPaprika API Docs",
      "twitter:description": "Explore CoinPaprika's API for real-time cryptocurrency market data."
    }
  },
  "styling": {
    "eyebrows": "breadcrumbs",
    "codeblocks": "dark",
    "appearance": {
      "default": "dark"
    },
    "css": [
      "./style.css"
    ]
  }
}