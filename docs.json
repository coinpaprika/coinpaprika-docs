{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "willow",
  "name": "CoinPaprika API",
  "colors": {
    "primary": "#CA312C",
    "dark": "#CA312C",
    "light": "#CA312C"
  },
  "background": {
  "color": {
    "light": "#ffffff",  
    "dark": "#252425"   
  }
},
"fonts": {
  "body": {
    "family": "Arial",
    "weight": 400
  },
  "heading": {
    "family": "Inconsolata",
    "weight": 700
  }
},
  "favicon": "/favicon.svg",
  "integrations": {
    "ga4": {
      "measurementId": "G-DO_USTAWIENIA"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude"
    ]
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "index",
              "faq"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "openapi": "api-reference/openapi.yml",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "api-reference/introduction"
            ]
          }
        ]
      },
      {
        "tab": "On-chain & DEX Data",
        "pages": [
          "dexpaprika"
        ]
      },
      {
        "tab": "Changelog",
        "pages": [
          "changelog"
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "API Playground",
          "href": "https://docs.coinpaprika.com/api-reference/introduction",
          "icon": "code"
        },
        {
          "anchor": "Discord Community",
          "href": "https://discord.gg/DhJge5TUGM",
          "icon": "discord"
        },
        {
          "anchor": "CoinPaprika",
          "href": "https://coinpaprika.com",
          "icon": "pepper-hot"
        },
        {
          "anchor": "CoinPaprika on X",
          "href": "https://x.com/coinpaprika",
          "icon": "x-twitter"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/api_logo.svg",
    "dark": "/logo/api_logo.svg",
    "href": "https://coinpaprika.com"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@coinpaprika.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Visit CoinPaprika",
      "href": "https://coinpaprika.com/"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/coinpaprika",
      "github": "https://github.com/coinpaprika"
    },
